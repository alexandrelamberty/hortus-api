{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "./",
  "modulePaths": ["<rootDir>"],
  "testEnvironment": "node",
  "testRegex": "spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
	"moduleNameMapper": {
    "^@Shared/(.)*$": "<rootDir>./$1"
  },
  "moduleDirectories": ["node_modules", "<rootDir>"]
}
